# Ignored keys for check-cluster-homogeneity.rb
# ---
# SYNTAX:
# sophia:
#   suno-[1-45]:
#     - ~main_memory.ram_size
#   sol-[1-50]:
#     - ~main_memory.ram_size
#   sol-29:
#     - +network_adapters.myri0
#
# nancy:
#   graphique: model differs on graphique-1
#   grisou-50+: 2 IB interfaces
#   talc-[15,95,106]: wrong BIOS version
#   graphene-[45,85]: wrong BIOS version
#   graphene-104+: different wattmeter
#   griffon-[1-91]: missing some serial
#   griffon-[11-14]: additional myrinet interface
---
nancy:
  graphique-[1-2]:
    - ~gpu.gpu_model
  grisou-50:
    - +network_adapters.ib0
  talc-[15-16]:
    - ~bios.release_date
    - ~bios.version
  talc-[95-96]:
    - ~bios.release_date
    - ~bios.version
  talc-[106-107]:
    - ~bios.release_date
    - ~bios.version
  graphene-[45-46]:
    - ~bios.release_date
    - ~bios.version
  graphene-[86-87]:
    - ~bios.release_date
    - ~bios.version
  graphene-[104-144]:
    - ~monitoring.wattmeter
  griffon-[1-91]:
    - +chassis.serial
    - -chassis.serial
  griffon-11:
    - +network_adapters.eth2
  griffon-15:
    - -network_adapters.eth2

rennes:
  parasilo-[1-28]:
    - ~storage_devices.sdf.model
    - ~storage_devices.sdf.rev
    - ~storage_devices.sd[a-e].by_id
    - ~storage_devices.sd[a-e].by_path
  paranoia-[1-8]:
    - ~storage_devices.sd[b-e].model
    - ~storage_devices.sd[b-e].rev
    - ~storage_devices.sd[a-e].by_id
    - ~storage_devices.sd[a-e].by_path
  parapide-[1-25]:
    - ~storage_devices.sd[a-e].by_id
    - ~storage_devices.sd[a-e].by_path
  parapluie-[1-40]:
    - ~storage_devices.sd[a-e].by_id
    - ~storage_devices.sd[a-e].by_path
  paravance-[1-72]:
    - ~storage_devices.sd[a-e].by_id
    - ~storage_devices.sd[a-e].by_path
sophia:
  sol-29:
    - +network_adapters.myri0
  suno-2:
    - +chassis.serial
  uva-13:
    - -chassis.serial
  uvb-3:
    - -chassis.serial
    - ~chassis.manufacturer
  uvb-4:
    - +chassis.serial
    - ~chassis.manufacturer
  uvb-13:
    - -chassis.serial
    - ~chassis.manufacturer
  uvb-14:
    - +chassis.serial
    - ~chassis.manufacturer
