<% 
  nodes_number = 72
  cluster_name = "dahu"
  no_ib_nodes = %w(18 27 30)
%>

---
model: Dell PowerEdge C6420
created_at: 2018-03-22 11:00:00 GMT
priority: '201800'
kavlan: true
keep_alive_nodes_count: 3
boot_type: bios
queues:
  - admin
  - default

nodes:
  <% (1..nodes_number).each { |i| %>
  <% if i > 32%>
  <%= cluster_name %>-<%= i %>:
    status: retired
  <%else%>
  <%= cluster_name %>-<%= i %>:
    processor:
      microarchitecture: Skylake-SP
      clock_speed: 2100000000
    software:
      standard-environment: debian11-x64-std
    management_tools:
      bmc_vendor_tool: racadm
    supported_job_types:
      deploy: true
      besteffort: true
      max_walltime: 0
    network_adapters:
      bmc:
        interface: Ethernet
        enabled: true
        mountable: false
        mounted: false
      eth0:
        mounted: true
        enabled: true
        mountable: true
      eth1:
        mounted: false
        enabled: false
        mountable: false
      <%unless no_ib_nodes.include?(i.to_s) %>
      ib0:
        mounted: true
        enabled: true
        mountable: true
        netmask: 255.255.240.0
      <% end %>
    storage_devices:
      pci-0000:00:11.5-ata-3:
        interface: SATA
        id: disk0
      pci-0000:00:11.5-ata-4:
        interface: SATA
        id: disk1
      pci-0000:00:11.5-ata-5:
        interface: SATA
        id: disk2
    nodeset: dahu
  <% end %>
  <% } %>
  <%# dahu-[33-72]:
    status: retired %>